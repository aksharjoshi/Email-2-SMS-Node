function isValidateEmail(email) {
		    var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
		    return re.test(email);
		}
		
function saveServices(){


	var serviceurl= "/updateservice";
		
	if($("#txtGmail").val() != ""){
	
		if(!(isValidateEmail($("#txtGmail").val()))){
		 	alert("Invalid Gmail Id");
		 	return false;
		 }
	}
	if($("#txtOutlook").val() != ""){
		if(!(isValidateEmail($("#txtOutlook").val()))){
		 	alert("Invalid Outlook Id");
		 	return false;
		 }
	}
	if($("#txtOther").val() != ""){
		if(!(isValidateEmail($("#txtOther").val()))){
		 	alert("Invalid Other Domain Id");
		 	return false;
		 }
	}
		
